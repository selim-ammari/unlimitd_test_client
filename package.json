    "@hookform/resolvers": "^2.8.5",
    "@mui/lab": "^5.0.0-alpha.60",
    "@mui/material": "^5.2.4",
    "eslint": "^7.32.0",
    "graphql": "^16.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.22.2",
